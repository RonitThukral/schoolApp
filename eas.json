{
  "build": {
    "android-preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "android-release": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "ios-preview": {
      "developmentClient": true
    },
    "ios-internal": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "distribution": "store"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "developmentClient": true
      },
      "android": {
        "buildType": "apk"
      }
    }
  }
}

